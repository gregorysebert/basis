<?xml version="1.0" encoding="UTF-8"?>
<bundle>
    <UIResultForm>
        <header>
            <Name>Name</Name>
            <User>File on</User>
            <Action>Action</Action>
        </header>
        <action>
            <title>
                <ViewNode>View document</ViewNode>
            </title>
            <Export>Export result</Export>
        </action>
    </UIResultForm>

    <UISearchBasisPortlet>
        <simpleSearch>Simple search</simpleSearch>
        <advancedSearch>Advanced search</advancedSearch>
        <Result>Result</Result>
    </UISearchBasisPortlet>

    <UISimpleSearchForm>
        <label>
            <NumberResult>Number results by page</NumberResult>
            <Query>Query</Query>
            <Language>Language</Language>
            <Attribut>Attribut</Attribut>
            <Date>Date</Date>
            <Date_second>2nd date</Date_second>
            <ListDate>Opertor</ListDate>
            <Query1>All folder where follow up editor is the current user</Query1>
            <Query2>All folder where follow up editor is a specific group</Query2>
            <Query3>All folder where follow up editor is a specific user</Query3>
            <Query4>All folder where follow up editor is a specific action</Query4>
            <Query5>All folder where id folder contains {?}</Query5>
            <Query6>All folder who has been created by the current user</Query6>
            <Query7>All folder who has been created by a specific user</Query7>
            <At>On</At>
            <Between>Between</Between>
        </label>
        <action>
            <Search>Search</Search>
            <Cancel>Cancel</Cancel>
        </action>
    </UISimpleSearchForm>

    <UIAdvancedSearchForm>
        <action>
            <Search>Search</Search>
            <Cancel>Cancel</Cancel>
        </action>
        <label>
            <From>List: </From>
            <Limited>Limited search (500 first results)</Limited>
        </label>
        <ListBox>
            <Folder>Folder</Folder>
            <Document>Document</Document>
            <Follow_folder>Follow folder</Follow_folder>
            <Follow_document>Follow document</Follow_document>
        </ListBox>
        <legend>
            <label>Legend</label>
            <historic1>&amp; = property 1 AND property 2 (ex: contains "000&amp;001")</historic1>
            <historic2># = property 1 OR property 2 (ex: contains "000#001")</historic2>
        </legend>
    </UIAdvancedSearchForm>

    <UIPropertyInputForm>
        <ListBox>
            <Equals>Equals</Equals>
            <Contains>Contains</Contains>
            <Not_Equals>Not Equals</Not_Equals>
            <Not_Contains>Not Contains</Not_Contains>
        </ListBox>
    </UIPropertyInputForm>

    <UIBasisFollowDocForm>
        <radiobox>
            <All>All</All>
            <Last>Last</Last>
        </radiobox>
        <legend>
            <Followdocument>Follow Document</Followdocument>
        </legend>
    </UIBasisFollowDocForm>

    <UIBasisFollowFolderForm>
        <radiobox>
            <All>All</All>
            <Last>Last</Last>
        </radiobox>
        <legend>
            <Followfolder>Follow Folder</Followfolder>
        </legend>
    </UIBasisFollowFolderForm>

    <UIBasisDocForm>
        <legend>
            <Document>Document</Document>
        </legend>
    </UIBasisDocForm>

    <UIBasisFolderForm>
        <legend>
            <Folder>Folder</Folder>
        </legend>
    </UIBasisFolderForm>


    <basisFolder>
        <label>
            <folderInternSender>RNN Name</folderInternSender>
        </label>
    </basisFolder>


</bundle>