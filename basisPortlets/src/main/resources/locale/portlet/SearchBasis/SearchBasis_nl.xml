<?xml version="1.0" encoding="UTF-8"?>
<bundle>
    <UIResultForm>
        <header>
            <Name>Naam</Name>
            <User>Dossier van</User>
            <Action>Actie</Action>
        </header>
        <action>
            <title>
                <ViewNode>Bekijk document</ViewNode>
            </title>
            <Export>Export resultaat</Export>
        </action>
    </UIResultForm>

    <UISearchBasisPortlet>
        <simpleSearch>Eenvoudige zoekopdracht</simpleSearch>
        <advancedSearch>Uitgebreid zoeken</advancedSearch>
        <Result>Resultaat</Result>
    </UISearchBasisPortlet>

    <UISimpleSearchForm>
        <label>
            <NumberResult>Aantal resultaten per pagina</NumberResult>
            <Query>Vraag</Query>
            <Language>Taal</Language>
            <Attribut>Attribuut</Attribut>
            <Date>Datum</Date>
            <Date_second>2e datum</Date_second>
            <ListDate>Operator</ListDate>
            <Query1>Alle map waarin follow-up-editor is de huidige gebruiker</Query1>
            <Query2>Alle map waarin follow-up-editor is een specifieke groep</Query2>
            <Query3>Alle map waarin follow-up-editor is een specifieke gebruiker</Query3>
            <Query4>Alle map waarin follow-up-editor is een specifieke actie</Query4>
            <Query5>Alle map waar id map bevat {?}</Query5>
            <Query6>Alle map die is gecreëerd door de huidige gebruiker</Query6>
            <Query7>Alle map die is gecreëerd door een specifieke gebruiker</Query7>
            <At>Op</At>
            <Between>Tussen</Between>
        </label>
        <action>
            <Search>Zoeken</Search>
            <Cancel>Annuleren</Cancel>
        </action>
    </UISimpleSearchForm>

    <UIAdvancedSearchForm>
        <action>
            <Search>Zoeken</Search>
            <Cancel>Annuleren</Cancel>
        </action>
        <label>
            <From>Lijst: </From>
            <Limited>Limited zoeken (500 eerste resultaten)</Limited>
        </label>
        <ListBox>
            <Folder>Dossier</Folder>
            <Document>Document</Document>
            <Follow_folder>Volg dossier</Follow_folder>
            <Follow_document>Volg document</Follow_document>
        </ListBox>
        <legend>
            <label>Legende</label>
            <historic1>&amp; = bezit 1 EN bezit 2 (ex: bevat "000&amp;001")</historic1>
            <historic2># = bezit 1 OF bezit 2 (ex: bevat "000#001")</historic2>
        </legend>
    </UIAdvancedSearchForm>

    <UIPropertyInputForm>
        <ListBox>
            <Equals>gelijk</Equals>
            <Contains>bevat</Contains>
            <Not_Equals>Niet gelijk</Not_Equals>
            <Not_Contains>Niet bevat</Not_Contains>
        </ListBox>
    </UIPropertyInputForm>

    <UIBasisFollowDocForm>
        <radiobox>
            <All>Alle</All>
            <Last>Laatste</Last>
        </radiobox>
        <legend>
            <Followdocument>Volg document</Followdocument>
        </legend>
    </UIBasisFollowDocForm>

    <UIBasisFollowFolderForm>
        <radiobox>
            <All>Alle</All>
            <Last>Laatste</Last>
        </radiobox>
        <legend>
            <Followfolder>Volg dossier</Followfolder>
        </legend>
    </UIBasisFollowFolderForm>

    <UIBasisDocForm>
        <legend>
            <Document>Document</Document>
        </legend>
    </UIBasisDocForm>

    <UIBasisFolderForm>
        <legend>
            <Folder>Dossier</Folder>
        </legend>
    </UIBasisFolderForm>


    <basisFolder>
        <label>
            <folderInternSender>RNN Naam</folderInternSender>
        </label>
    </basisFolder>
</bundle>