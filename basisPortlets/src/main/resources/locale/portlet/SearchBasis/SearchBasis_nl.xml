        <legend>
            <Followfolder>Volg dossier</Followfolder>
        </legend>
    </UIBasisFollowFolderForm>

    <UIBasisDocForm>
        <legend>
            <Document>Document</Document>
        </legend>
    </UIBasisDocForm>

    <UIBasisFolderForm>
        <legend>
            <Folder>Dossier</Folder>
        </legend>
