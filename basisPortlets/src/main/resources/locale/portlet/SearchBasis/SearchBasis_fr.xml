<?xml version="1.0" encoding="UTF-8"?>
<bundle>
    <UIResultForm>
        <header>
            <Name>Nom</Name>
            <Path>Chemin</Path>
            <Action>Action</Action>
        </header>
        <label>
            <View>Voir document</View>
            <NoResult>Pas de résultat!</NoResult>
        </label>
    </UIResultForm>

    <UISearchBasisPortlet>
        <simpleSearch>Recherche simple</simpleSearch>
        <advancedSearch>Recherche avancée</advancedSearch>
        <Result>Résultat</Result>
    </UISearchBasisPortlet>

    <UISimpleSearchForm>
        <label>
            <Query>Requête</Query>
            <Language>Langue</Language>
            <Attribut>Attribut</Attribut>
            <Query1>Tout les dossiers où l'éditeur de suivi est l'utilisateur courant</Query1>
            <Query2>Tout les dossiers où l'éditeur de suivi est équal à un groupe spécifique</Query2>
            <Query3>Tout les dossiers où l'éditeur de suivi est équal à un utilisateur spécifique</Query3>
            <Query4>Tout les dossiers où l'éditeur de suivi est équal à une action spécifique</Query4>
        </label>
        <action>
            <Search>Rechercher</Search>
            <Cancel>Annuler</Cancel>
        </action>
    </UISimpleSearchForm>

    <UIAdvancedSearchForm>
        <action>
            <Search>Rechercher</Search>
            <Cancel>Annuler</Cancel>
        </action>
        <label>
            <From>Liste:</From>
        </label>
        <ListBox>
            <Folder>Dossier</Folder>
            <Document>Document</Document>
            <Follow_folder>Suivi de dossier</Follow_folder>
            <Follow_document>Suivi de document</Follow_document>
        </ListBox>
    </UIAdvancedSearchForm>

    <UIPropertyInputForm>
        <ListBox>
            <Equals>Egal</Equals>
            <Contains>Contient</Contains>
            <Not_Equals>Pas égal</Not_Equals>
            <Not_Contains>Ne contient pas</Not_Contains>
        </ListBox>
    </UIPropertyInputForm>


    <UIBasisFollowDocForm>
        <radiobox>
            <All>Tout</All>
            <Last>Dernier</Last>
        </radiobox>
        <legend>
            <Followdocument>Suivi de document</Followdocument>
        </legend>
    </UIBasisFollowDocForm>

    <UIBasisFollowFolderForm>
        <radiobox>
            <All>Tout</All>
            <Last>Dernier</Last>
        </radiobox>
        <legend>
            <Followfolder>Suivi de dossier</Followfolder>
        </legend>
    </UIBasisFollowFolderForm>

    <UIBasisDocForm>
        <legend>
            <Document>Document</Document>
        </legend>
    </UIBasisDocForm>

    <UIBasisFolderForm>
        <legend>
            <Folder>Dossier</Folder>
        </legend>
    </UIBasisFolderForm>

    <basisFolder>
        <label>
            <folderInternSender>Expéditeur interne</folderInternSender>
        </label>
    </basisFolder>
</bundle>