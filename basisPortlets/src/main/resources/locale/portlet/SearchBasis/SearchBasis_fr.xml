<?xml version="1.0" encoding="UTF-8"?>
<bundle>
    <UIResultForm>
        <header>
            <Name>Nom</Name>
            <User>Dossier de</User>
            <Action>Action</Action>
        </header>
        <action>
            <title>
                <ViewNode>Voir document</ViewNode>
            </title>
            <Export>Exporter résultat</Export>
        </action>
    </UIResultForm>

    <UISearchBasisPortlet>
        <simpleSearch>Recherche simple</simpleSearch>
        <advancedSearch>Recherche avancée</advancedSearch>
        <Result>Résultat</Result>
    </UISearchBasisPortlet>

    <UISimpleSearchForm>
        <label>
            <NumberResult>Nombre de résultats par page</NumberResult>
            <Query>Requête</Query>
            <Language>Langue</Language>
            <Attribut>Attribut</Attribut>
            <Date>Date</Date>
            <Date_second>2ème date</Date_second>
            <ListDate>Opérateur</ListDate>
            <Query1>Tout les dossiers où l'éditeur de suivi est l'utilisateur courant</Query1>
            <Query2>Tout les dossiers où l'éditeur de suivi est égal à un groupe spécifique</Query2>
            <Query3>Tout les dossiers où l'éditeur de suivi est égal à un utilisateur spécifique</Query3>
            <Query4>Tout les dossiers où l'éditeur de suivi est égal à une action spécifique</Query4>
            <Query5>Tout les dossiers où l'id du dossier contient {?}</Query5>
            <Query6>Tout les dossiers qui a été créé par l'utilisateur courant</Query6>
            <Query7>Tout les dossiers qui a été créé par un utilisateur spécifique</Query7>
            <At>Le</At>
            <Between>Entre</Between>
        </label>
        <action>
            <Search>Rechercher</Search>
            <Cancel>Annuler</Cancel>
        </action>
    </UISimpleSearchForm>

    <UIAdvancedSearchForm>
        <action>
            <Search>Rechercher</Search>
            <Cancel>Annuler</Cancel>
        </action>
        <label>
            <From>Liste:</From>
            <Limited>Recherche limitée (500 premiers résultats)</Limited>
        </label>
        <ListBox>
            <Folder>Dossier</Folder>
            <Document>Document</Document>
            <Follow_folder>Suivi de dossier</Follow_folder>
            <Follow_document>Suivi de document</Follow_document>
        </ListBox>
        <legend>
            <label>Légende</label>
            <historic1>&amp; = Propriété 1 ET propriété 2 (ex: contient "000&amp;001")</historic1>
            <historic2># = Propriété 1 OU propriété 2 (ex: contient "000#001")</historic2>
        </legend>
    </UIAdvancedSearchForm>

    <UIPropertyInputForm>
        <ListBox>
            <Equals>Egal</Equals>
            <Contains>Contient</Contains>
            <Not_Equals>Pas égal</Not_Equals>
            <Not_Contains>Ne contient pas</Not_Contains>
        </ListBox>
    </UIPropertyInputForm>


    <UIBasisFollowDocForm>
        <radiobox>
            <All>Tout</All>
            <Last>Dernier</Last>
        </radiobox>
        <legend>
            <Followdocument>Suivi de document</Followdocument>
        </legend>
    </UIBasisFollowDocForm>

    <UIBasisFollowFolderForm>
        <radiobox>
            <All>Tout</All>
            <Last>Dernier</Last>
        </radiobox>
        <legend>
            <Followfolder>Suivi de dossier</Followfolder>
        </legend>
    </UIBasisFollowFolderForm>

    <UIBasisDocForm>
        <legend>
            <Document>Document</Document>
        </legend>
    </UIBasisDocForm>

    <UIBasisFolderForm>
        <legend>
            <Folder>Dossier</Folder>
        </legend>
    </UIBasisFolderForm>

    <basisFolder>
        <label>
            <folderInternSender>RNN Nom</folderInternSender>
        </label>
    </basisFolder>
</bundle>